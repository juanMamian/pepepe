ado\"},on:{click:function(e){return e.stopPropagation(),o.toggleDesplegar(e)}}},[i(\"img\",{directives:[{name:\"show\",rawName:\"v-show\",value:!o.desplegado&&!o.waitingFetchChildren,expression:\"!desplegado && !waitingFetchChildren\"}],class:{transparentoso:o.transparentoso},staticStyle:{filter:\"var(--atlasFilterGris)\"},attrs:{src:n(\"dc76\"),alt:\"Plegado\",title:\"Desplegar\"}}),i(\"img\",{directives:[{name:\"show\",rawName:\"v-show\",value:o.desplegado&&!o.waitingFetchChildren,expression:\"desplegado && !waitingFetchChildren\"}],class:{transparentoso:o.transparentoso},staticStyle:{filter:\"var(--atlasFilterGris)\"},attrs:{src:n(\"3f4b\"),alt:\"Desplegado\",title:\"Plegar\"}}),i(\"loading\",{directives:[{name:\"show\",rawName:\"v-show\",value:o.waitingFetchChildren,expression:\"waitingFetchChildren\"}],attrs:{texto:\"\"}})],1),i(\"div\",{class:{transparentoso:o.transparentoso,completa:o.seleccionado||o.requeridoPorSeleccionado||o.requiereAlSeleccionado},style:[o.estiloCartelNombre,o.colorCartelNombre],attrs:{id:\"cartelNombre\"}},[i(\"div\",{attrs:{id:\"nombre\",draggable:\"false\"}},[o._v(\" \"+o._s(o.callingPosiciones?o.esteNodo.fuerzaColision.fuerza.toFixed(2)+\"-\"+o.esteNodo.fuerzaColision.direccion.toFixed(2):o.esteNodo.nombre)+\" \")])]),i(\"div\",{directives:[{name:\"show\",rawName:\"v-show\",value:o.menuCx,expression:\"menuCx\"}],attrs:{id:\"menuContextual\"},on:{mouseup:function(o){o.stopPropagation()},mousedown:function(o){o.stopPropagation()}}},[o.usuarioResponsableAmplio?[i(\"div\",{staticClass:\"tituloSeccionMenuCx\"},[o._v(\"Crear nuevo...\")]),i(\"div\",{staticClass:\"botonesSeccionMenuCx\"},[i(\"div\",{staticClass:\"botonMenuCx\",class:{deshabilitado:o.creandoSubNodo},on:{click:function(e){return e.stopPropagation(),o.crearNuevoNodoUnder({tipoNodo:\"trabajo\"})}}},[o._v(\" Trabajo \")]),\"trabajo\"!=o.esteNodo.tipoNodo?i(\"div\",{staticClass:\"botonMenuCx\",class:{deshabilitado:o.creandoSubNodo},on:{click:function(e){return e.stopPropagation(),