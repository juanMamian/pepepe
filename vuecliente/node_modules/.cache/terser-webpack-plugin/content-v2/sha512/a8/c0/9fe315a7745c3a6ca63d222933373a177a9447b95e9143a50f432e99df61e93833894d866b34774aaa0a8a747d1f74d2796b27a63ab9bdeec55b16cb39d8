olesConjuntos\"}},[i(\"div\",{staticClass:\"botonControlesConjuntos\",on:{click:o.crearNuevaColeccion}},[o._v(\"Nueva coleccin\")])])],2),i(\"div\",{style:[{visibility:null!=o.idNodoSeleccionado?\"visible\":\"hidden\"}],attrs:{id:\"controlesListaNodos\"}},[i(\"div\",{staticClass:\"controlListaNodos hoverGris\",on:{click:function(e){return e.stopPropagation(),o.$emit(\"centrarEnNodo\",o.idNodoSeleccionado)}}},[o._v(\" Centrar \")]),i(\"img\",{directives:[{name:\"show\",rawName:\"v-show\",value:o.idNodoSeleccionado,expression:\"idNodoSeleccionado\"}],staticClass:\"botonEliminarNodoColeccion\",class:{deshabilitado:o.enviandoQueryNodosSeccion},attrs:{src:n(\"2191\"),alt:\"Eliminar\"},on:{click:function(e){return e.stopPropagation(),o.eliminarNodoSeleccionadoSeccionSeleccionada(e)}}})]),o.conjuntoSeleccionado?i(\"div\",{directives:[{name:\"show\",rawName:\"v-show\",value:o.abierto&&o.conjuntoSeleccionado,expression:\"abierto && conjuntoSeleccionado\"}],key:\"listaNodos\"+o.idConjuntoSeleccionado,attrs:{id:\"listaNodosConjunto\"},on:{click:function(e){if(e.target!==e.currentTarget)return null;e.stopPropagation(),o.idNodoSeleccionado=null}}},o._l(o.conjuntoSeleccionado.nodos,(function(e){return i(\"icono-nodo-conocimiento\",{directives:[{name:\"show\",rawName:\"v-show\",value:o.conjuntoSeleccionado,expression:\"conjuntoSeleccionado\"}],key:e.id,attrs:{esteNodo:e,seleccionado:e.id===o.idNodoSeleccionado},nativeOn:{click:function(n){n.stopPropagation(),o.idNodoSeleccionado=e.id},dblclick:function(n){return n.stopPropagation(),o.$emit(\"centrarEnNodo\",e.id)}}})})),1):o._e(),o.conjuntoSeleccionado?i(\"div\",{directives:[{name:\"show\",rawName:\"v-show\",value:\"nodosConClaseDictada\"===o.conjuntoSeleccionado.nombre,expression:\"conjuntoSeleccionado.nombre === 'nodosConClaseDictada'\"}],attrs:{id:\"listaClasesNodo\"}},o._l(o.clasesDictadasUsuario,(function(e){return i(\"div\",{key:\"infoClase\"+e.id,staticClass:\"infoClase\",on:{click:function(n){return n.stopPropagation(),o.$