AAA,GAAA,CAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AACA,SAAA,OAAA,GAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AACA,IAAA,OAAA,CAAA,GAAA;AACA,SAAA,oBAAA,GAAA,KAAA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n\n  \n\n  <div class=\"libro\">\n    <pagina\n      v-for=\"(pagina, index) of paginasOrdenadas\"\n      :key=\"pagina.id\"\n      :paginaDerecha=\"index % 2 > 0 || index == 0\"\n      :estaPagina=\"pagina\"\n      :idLibro=\"idLibro\"\n      :seleccionada=\"idPaginaSeleccionada==pagina.id\"\n      @click.native=\"idPaginaSeleccionada=pagina.id\"\n    />\n\n    <div id=\"bCrearNuevaPagina\">Crear nueva p√°gina</div>\n  </div>\n</template>\n\n<script>\nimport gql from \"graphql-tag\";\nimport Pagina from \"./Pagina.vue\";\n\nconst QUERY_LIBRO = gql`\n  query($idLibro: ID!) {\n    libro(idLibro: $idLibro) {\n      id\n      idsEditores\n      paginas {\n        id\n        numPag\n        color\n        cuadrosTexto {\n          id\n          texto\n          posicion {\n            x\n            y\n          }\n          size {\n            x\n            y\n          }\n          audio {\n            tipoReproduccion\n          }\n          formato {\n            fontSize\n            colorLetra\n            tipoLetra\n            alineacion\n          }\n        }\n        cuadrosImagen {\n          id\n          tipoActivacionSecundario\n          posicion {\n            x\n            y\n          }\n          size {\n            x\n            y\n          }\n          audio {\n            tipoReproduccion\n          }\n        }\n      }\n    }\n  }\n`;\n\nexport default {\n  components: { Pagina },\n  name: \"Libro\",\n  apollo: {\n    libro: {\n      query: QUERY_LIBRO,\n      variables() {\n        return {\n          idLibro: this.idLibro,\n        };\n      },\n      skip() {\n        return this.skipearDownloadLibro || this.idLibro==null;\n      },\n    },\n  },\n  data() {\n    return {\n      skipearDownloadLibro: true,\n      libro: {\n        paginas: null,\n      },\n      idLibro: null,\n      idPaginaSeleccionada:null,\n    };\n  },\n  computed: {\n    paginasOrdenadas() {\n      if (this.libro.paginas) {\n        var aPags = this.libro.paginas;\n        return aPags.sort((a, b) => {\n          return parseInt(a.numPag) - parseInt(b.numPag);\n        });\n      }\n      return [];\n    },\n  },\n  mounted() {\n    let uri = window.location.search.substring(1);\n    let params = new URLSearchParams(uri);\n    console.log(params.get(\"id\"));\n    this.idLibro = params.get(\"id\");\n    console.log(`Des-skipeando libro query`);\n    this.skipearDownloadLibro = false;\n  },\n};\n</script>\n\n<style scoped>\n.libro {\n  margin-top:100px;\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  row-gap: 20px;\n}\n\n#bCrearNuevaPagina{\n  padding: 5px;\n  border: 1px solid rgb(65, 0, 65);\n  cursor: pointer;\n  align-self:center ;\n  justify-self: center;\n  border-radius: 5px;\n}\n\n#bCrearNuevaPagina:hover{\n  background-color: rgb(182, 105, 182);\n}\n</style>"],"sourceRoot":"src/components"}]}